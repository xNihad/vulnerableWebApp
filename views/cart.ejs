<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cart</title>
</head>
<body>
    <h2>Your Cart</h2>
    <ul>
        <% cartItems.forEach(item => { %>
            <li><%= item.product_name %> - Quantity: <%= item.quantity %></li>
        <% }); %>
    </ul>
</body>
</html>
